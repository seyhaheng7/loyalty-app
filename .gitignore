# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore Mac DS_Store files
.DS_Store
.keep
/public/uploads/*
/public/assets/*
/public/apidocs/api/v1
/public/apidocs/api-docs.json

/node_modules
/yarn-error.log

.byebug_history
.env
